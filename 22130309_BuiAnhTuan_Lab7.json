{
  "clickpaths": {
    "AddToCart01": {
      "name": "AddToCart01 - Thêm 1 sản phẩm (mặc định quantity = 1)",
      "configuration": {
        "device": {
          "orientation": "landscape",
          "deviceName": "Desktop"
        }
      },
      "type": "clickpath",
      "version": "1.0",
      "events": [
        {
          "description": "Loading of \"https://cms.anhtester.com/product/lenovo-11-2024-04-7\"",
          "wait": { "waitFor": "page_complete" },
          "target": {},
          "type": "navigate",
          "url": "https://cms.anhtester.com/product/lenovo-11-2024-04-7"
        },
        {
          "description": "click Add to Cart button",
          "wait": { "waitFor": "page_complete" },
          "target": {
            "locators": [
              {
                "type": "dom",
                "value": "document.evaluate('(.//*[normalize-space(text()) and normalize-space(.)=\\'$10,000,000.00\\'])[2]/following::span[1]', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
              }
            ]
          },
          "type": "click",
          "button": 0
        },
        {
          "description": "click Proceed to Checkout",
          "wait": { "waitFor": "page_complete" },
          "target": {
            "locators": [
              { "type": "css", "value": "a:contains(\"Proceed to Checkout\")" }
            ]
          },
          "type": "click",
          "button": 0
        },
        {
          "description": "Loading cart page",
          "wait": { "waitFor": "page_complete" },
          "target": {},
          "type": "navigate",
          "url": "https://cms.anhtester.com/cart"
        }
      ]
    },

    "AddToCart02": {
      "name": "AddToCart02 - Nhập số lượng = 0 (kiểm tra validation)",
