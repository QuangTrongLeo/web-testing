        { "description": "click Add to Cart", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "dom", "value": "document.evaluate('(.//*[normalize-space(text()) and normalize-space(.)=\\'$10,000,000.00\\'])[2]/following::span[1]', document, null, XPathResult.ANY_TYPE, null).iterateNext()" }] }, "type": "click", "button": 0 },
        { "description": "click Continue Shopping (nếu có modal)", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "dom", "value": "document.evaluate('//div[@id=\\'addToCart-modal-body\\']/div/div[2]/button', document, null, XPathResult.ANY_TYPE, null).iterateNext()" }] }, "type": "click", "button": 0 }
      ]
    },

