    "Newest": {
      "name": "Newest - Sắp xếp sản phẩm theo Newest",
      "configuration": { "device": { "orientation": "landscape", "deviceName": "Desktop" } },
      "type": "clickpath",
      "version": "1.0",
      "events": [
        { "description": "Loading search page", "wait": { "waitFor": "page_complete" }, "target": {}, "type": "navigate", "url": "https://cms.anhtester.com/search?keyword=lap&brand=&sort_by=oldest&min_price=&max_price=" },
        { "description": "click sort dropdown", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "dom", "value": "document.evaluate('//form[@id=\\'search-form\\']/div/div[2]/div/div/div[4]/div/button', document, null, XPathResult.ANY_TYPE, null).iterateNext()" }] }, "type": "click", "button": 0 },
        { "description": "click Newest option", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "css", "value": "#bs-select-2-0" }] }, "type": "click", "button": 0 },
        { "description": "select Newest via JS", "wait": { "waitFor": "page_complete" }, "target": {}, "type": "javascript", "javaScript": "let add = false; let x = typeof $ !== 'undefined' ? $('[name=\"sort_by\"]')[0] : document.querySelector('[name=\"sort_by\"]'); for (i = 0; i < x.options.length; i++) { if (x.options[i].text === \"Newest\") { if(add) { x.options[i].selected = true; } else { x.options.selectedIndex = i; } } }" },
        { "description": "Loading newest sorted page", "wait": { "waitFor": "page_complete" }, "target": {}, "type": "navigate", "url": "https://cms.anhtester.com/search?keyword=lap&brand=&sort_by=newest&min_price=&max_price=" }
